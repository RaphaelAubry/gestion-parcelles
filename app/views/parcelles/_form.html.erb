<%= form_for @parcelle do |form| %>
  <input-group>
    <%= form.label :reference_cadastrale %>
    <%= form.text_field :reference_cadastrale %>
  </input-group>
  <input-group>
    <%= form.label :lieu_dit %>
    <%= form.text_field :lieu_dit %>
  </input-group>
  <input-group>
    <%= form.label :code_officiel_geographique %>
    <%= form.text_field :code_officiel_geographique %>
  </input-group>
  <input-group>
    <%= form.label :surface %>
    <%= form.text_field :surface %>
  </input-group>
  <input-group>
    <%= form.label :annee_plantation %>
    <%= form.text_field :annee_plantation %>
  </input-group>
    <input-group>
    <%= form.label :distance_rang%>
    <%= form.text_field :distance_rang %>
  </input-group>
  <input-group>
    <%= form.label :distance_pieds %>
    <%= form.text_field :distance_pieds %>
  </input-group>
  <button-group>
    <%= form.submit 'valider', class: 'button approuve' %>
    <%= link_to :annuler, :back %>
  </button-group>
<% end %>
